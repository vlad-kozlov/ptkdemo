{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ptkadf"
		},
		"integrationRuntime_integrationRuntimePTKdemo": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "integrationRuntimePTKdemo"
		},
		"AzureDataLakeStoragePTKDemo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStoragePTKDemo'"
		},
		"SqlServerLocal_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLocal'"
		},
		"SqlServerLocal_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerLocal'"
		},
		"AzureDataLakeStoragePTKDemo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ptkdemoadls2.dfs.core.windows.net"
		},
		"SqlServerLocal_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vkwindows10\\vlkozlov"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"integrationRuntime_integrationRuntimePTKdemo": {
						"value": "[parameters('integrationRuntime_integrationRuntimePTKdemo')]"
					},
					"AzureDataLakeStoragePTKDemo_accountKey": {
						"value": "[parameters('AzureDataLakeStoragePTKDemo_accountKey')]"
					},
					"SqlServerLocal_connectionString": {
						"value": "[parameters('SqlServerLocal_connectionString')]"
					},
					"SqlServerLocal_password": {
						"value": "[parameters('SqlServerLocal_password')]"
					},
					"AzureDataLakeStoragePTKDemo_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragePTKDemo_properties_typeProperties_url')]"
					},
					"SqlServerLocal_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLocal_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}